package com.hibernate.model;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name = "tbl_Sales_Detail")
public class Sales_detail {
	@Id @GeneratedValue
	@Column(name = "id")
	private String id; 
	@Column(name = "sellID")
	private String sellID; 
	@Column(name = "spID")
	private String spID;
	@Column(name = "dj")
	private int dj;
	@Column(name = "sl")
	private int sl;
	
	public Sales_detail(){}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getSellID() {
		return sellID;
	}

	public void setSellID(String sellID) {
		this.sellID = sellID;
	}

	public String getSpID() {
		return spID;
	}

	public void setSpID(String spID) {
		this.spID = spID;
	}

	public int getDj() {
		return dj;
	}

	public void setDj(int dj) {
		this.dj = dj;
	}

	public int getSl() {
		return sl;
	}

	public void setSl(int sl) {
		this.sl = sl;
	}

}
